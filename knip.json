{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "app/**/*.{ts,tsx}",
    "components/**/*.{ts,tsx}",
    "lib/**/*.{ts,tsx}"
  ],
  "project": [
    "**/*.{ts,tsx,js,jsx,mjs}"
  ],
  "ignore": [
    ".next/**",
    "node_modules/**",
    "public/**",
    "old-infrastructure/**"
  ],
  "ignoreDependencies": [
    "tailwindcss"
  ],
  "ignoreExportsUsedInFile": true
}
